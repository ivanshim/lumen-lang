# Convert x to a string with value_to_string(x) and emit without a newline
fn write(x)
    emit(value_to_string(x))

# Write x followed by a newline
fn print(x)
    write(x)
    emit("\n")


# Print error message and halt execution
fn error(msg)
    print("ERROR: " . msg)
    1 / 0  # Halt execution via division by zero
