print("Test: let and let mut Keywords")

let x = 10
print("let x = 10")
print(x)

let mut y = 5
print("let mut y = 5")
print(y)

y = 20
print("After y = 20:")
print(y)

let x = 100
print("After let x = 100 (shadowing):")
print(x)

let result = x + y
print("let result = x + y")
print(result)

fn test_let()
    let a = 42
    let mut b = 10
    b = 50
    a + b

print("test_let():")
print(test_let())
