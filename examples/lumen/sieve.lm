write("Primes up to 1000: ")

n = 1000
count = 0

i = 2
while i <= n
    is_prime = true
    j = 2
    while j * j <= i
        if i % j == 0
            is_prime = false
        j = j + 1

    if is_prime
        write(i)
        count = count + 1
        if i < n
            write(", ")

    i = i + 1

print("")
write("Total primes found: ")
print(count)
