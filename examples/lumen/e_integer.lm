precision = 116000
scale = 1

i = 0
while i < precision
    scale = scale * 10
    i = i + 1

sum = scale
fact = 1
n = 1

while 1 == 1
    term = scale / fact
    if term == 0
        break
    sum = sum + term
    n = n + 1
    fact = fact * n

    if n % 10 == 0
        print(sum)

write("Final: ")
print(sum)
