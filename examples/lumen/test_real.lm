# Test Real numeric type
# Basic real() conversions and operations

# Test 1: real(integer)
print("Test 1: real(integer)")
x = real(5)
print(x)

# Test 2: real(rational)
print("Test 2: real(rational)")
y = real(1/3)
print(y)

# Test 3: real(rational, precision)
print("Test 3: real(1/3, 50)")
z = real(1/3, 50)
print(z)

# Test 4: int(real)
print("Test 4: int(real(3.9)) should be 3")
result = int(real(10/3))
print(result)

# Test 5: Comparisons
print("Test 5: Comparisons")
a = real(2)
b = 2
if a == b
    print("real(2) == 2: true")
else
    print("real(2) == 2: false")

# Test 6: Type introspection (might not be available, but try)
print("Test 6: str(real)")
r = real(22/7)
print(r)
