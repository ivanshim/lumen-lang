# ===== PARAMETER: Edit this value to compute Fibonacci at a different index =====
fib_index = 10000
# ==============================================================================

MEMOIZATION = true

fn fib(n)
    if n <= 1
        n
    else
        fib(n - 1) + fib(n - 2)

i = 0
result = 0
while i <= fib_index
    result = fib(i)
    i = i + 1

write("Fibonacci number at index ")
write(fib_index)
write(": ")
print(result)
