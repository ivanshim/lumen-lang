# Test cases for the round() function
# Demonstrates rounding with various precisions and numbers
# Uses computed rationals since Lumen doesn't support float literals

write("Positive number tests:")
write("round(1235/1000, 2) = ")
print(round(1235 / 1000, 2))

write("round(1234/1000, 2) = ")
print(round(1234 / 1000, 2))

write("round(1245/1000, 2) = ")
print(round(1245 / 1000, 2))

write("round(3/2, 0) = ")
print(round(3 / 2, 0))

write("round(5/2, 0) = ")
print(round(5 / 2, 0))

write("\nNegative number tests (round half away from zero):")
write("round(-1235/1000, 2) = ")
print(round(-1235 / 1000, 2))

write("round(-1234/1000, 2) = ")
print(round(-1234 / 1000, 2))

write("round(-1245/1000, 2) = ")
print(round(-1245 / 1000, 2))

write("round(-3/2, 0) = ")
print(round(-3 / 2, 0))

write("round(-5/2, 0) = ")
print(round(-5 / 2, 0))

write("\nBoundary cases:")
write("round(0, 2) = ")
print(round(0, 2))

write("round(5/1000, 2) = ")
print(round(5 / 1000, 2))

write("round(4/1000, 2) = ")
print(round(4 / 1000, 2))

write("round(10999/1000, 2) = ")
print(round(10999 / 1000, 2))

write("round(99995/1000, 2) = ")
print(round(99995 / 1000, 2))

write("\nMultiple decimal places with pi:")
write("round(314159/100000, 2) = ")
print(round(314159 / 100000, 2))

write("round(314159/100000, 3) = ")
print(round(314159 / 100000, 3))

write("round(314159/100000, 4) = ")
print(round(314159 / 100000, 4))
